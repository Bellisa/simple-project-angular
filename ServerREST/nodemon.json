      ".git",
      "node_modules/**/node_modules",
      "dist"
    ],
    "verbose": true,
    "execMap": {
      "js": "node"
    },
  
    "runOnChangeOnly": false,
    "watch": [
      "src/resources/**/*.js"
    ],
    "delay": 2500,
    "ext": "js,json"
  }